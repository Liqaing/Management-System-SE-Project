{
  "version": 2,
  "builds": [
    {
      "src": "src/server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["prisma/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/server.js"
    },
    {
      "src": "/doc",
      "dest": "src/server.js" 
    },
    {
      "src": "/(.*)",
      "dest": "src/server.html"
    }
  ]
}
